{{#if hasPrevious}}
  <a {{action 'backward'}} href="#" class='button pager__previous qa-pager-prev'>
    <i class="fa fa-step-backward"></i>
  </a>
{{/if}}
{{#if hasNext}}
  <a {{action 'forward'}} href="#" class='button pager__next qa-pager-next'>
    <i class="fa fa-step-forward"></i>
  </a>
{{/if}}
